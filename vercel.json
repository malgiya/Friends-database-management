{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }
  {
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    }
  }
}
